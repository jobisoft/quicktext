    <em:translator>Alexey Sinitsyn (ru)</em:translator>
    <em:optionsURL>chrome://quicktext/content/settings.xul</em:optionsURL>
    <em:iconURL>chrome://quicktext/skin/icon.png</em:iconURL>
    <em:homepageURL>https://github.com/thundernest/quicktext</em:homepageURL>

